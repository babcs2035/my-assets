    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-day-picker": "^8.10.1",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@tailwindcss/postcss": "^4",
